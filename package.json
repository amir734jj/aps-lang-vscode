{
    "name": "aps-lang",
    "displayName": "aps-lang",
    "description": "APS language support",
    "publisher": "hesamian",
    "version": "0.0.4",
    "repository": "https://github.com/amir734jj/aps-lang-vscode",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "aps",
                "aliases": [
                    "aps"
                ],
                "extensions": [
                    ".aps"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "aps",
                "scopeName": "",
                "path": "./syntaxes/aps.tmLanguage.json"
            }
        ]
    }
}
